<view class='my'>
  <view class='my-1'>
    <view class='my-1-1'>
      <view class='left'>
        <text>订单</text>
      </view>
      <view class='right'>
        <text bindtap='GoOrder'>更多订单</text>
      </view>
    </view>

    <view class='my-1-2'>
      <view class='box' bindtap='GoOrder'>
        <image src='http://zq2.51wdxh.com/jt-img/dfh.png'></image>
        <text>待发货</text>
      </view>

      <view class='box' bindtap='GoOrder'>
        <image src='http://zq2.51wdxh.com/jt-img/yfh.png'></image>
        <text>已发货</text>
      </view>

      <view class='box' bindtap='GoOrder'>
        <image src='http://zq2.51wdxh.com/jt-img/ywc.png'></image>
        <text>已完成</text>
      </view>
    </view>


  </view>

  <view class='my-2'>
    <view class='my-2-list' bindtap='GoAddress'>
      <text class='my-2-list-left'>
        <text>收货地址管理</text>
      </text>
      <image class='my-2-list-right' src='http://zq2.51wdxh.com/jt-img/jt.png'></image>
    </view>

     <view class='my-2-list'  bindtap='GoZc'>
      <text class='my-2-list-left'>
        <text>绑定手机号</text>
        <text class='sjh'>13123456789</text>
      </text>
      <image class='my-2-list-right' src='http://zq2.51wdxh.com/jt-img/jt.png'></image>
    </view>


     <view class='my-2-list'>
      <text class='my-2-list-left'>
        <text>客户电话</text>
      </text>
      <image class='my-2-list-right' src='http://zq2.51wdxh.com/jt-img/jt.png'></image>
    </view>
  </view>

</view>