page{
  background: #f6f5f5
}
.address-box{
  width: 100%;
  min-height: 300rpx;
  background: white
}
.address-box-1{
  width: 100%;
  height: 50rpx
}
.left{
  float: left;
  font-size: 30rpx;
  margin-left: 20rpx;
}
.right{
  float: right;
  font-size: 30rpx;
  margin-right: 20rpx
}
.address-box-2{
  width: 100%;
  height: 150rpx;
  border-bottom: 1px solid #f6f5f5
}
.address-box-2 text{
  font-size: 30rpx;
  /* margin-left: 20rpx; */
  width: 95%;
  margin: auto;
  display: block;
  color: #999999;
  padding-top: 20rpx
}
.address-left{
  float: left
}
.address-left icon{
  margin: 30rpx 20rpx;
  display: block;
  float: left
}
.address-left text{
  font-size: 30rpx;
  line-height: 100rpx
}
.address-right{
  float: right;
  margin-top: 30rpx;
  margin-right: 15rpx
}
.xg{
  display: block;
  width: 100rpx;
  height: 40rpx;
  line-height: 40rpx;
  background: #6cb8b4;
  font-size: 30rpx;
  text-align: center;
  color: white;
  float: left;
  margin-right: 20rpx
}
.del{
  display: block;
  width: 100rpx;
  height: 40rpx;
  line-height: 40rpx;
  background: #999999;
  font-size: 30rpx;
  text-align: center;
  color: white;
  float: left
}
.btn{
  width:  95%;
  height: 75rpx;
  background: #6db9b5;
  border-radius: 60rpx;
  margin: 30rpx auto;
}
.btn text{
  line-height: 75rpx;
  color: white;
  font-size: 30rpx;
  text-align: center;
  display: block
}